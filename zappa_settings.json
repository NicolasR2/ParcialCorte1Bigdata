{
    "dev": {
        "app_function": "proyecto.app",
        "aws_region": "us-east-1",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "profile_name": "default",
        "project_name": "casas",
        "runtime": "python3.10",
        "s3_bucket": "zappa-kpk1mm5he",
        "keep_warm": false,
        "apigateway_enabled": false,
        "manage_roles": false,
        "role_name": "LabRole",
        "events": [
            {
                "function": "proyecto.app",
                "expression": "cron(23 1 * * ? *)"
            }
        ] ,
        "environment_variables": {
            "AWS_ACCESS_KEY_ID": "${{ secrets.AWS_ACCESS_KEY_ID }}",
            "AWS_SECRET_ACCESS_KEY": "${{ secrets.AWS_SECRET_ACCESS_KEY }}",
            "AWS_SESSION_TOKEN": "${{ secrets.AWS_SESSION_TOKEN }}"
        }
    },

    "dev2": {
        "app_function": "proyecto1.app",
        "aws_region": "us-east-1",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "project_name": "lambda_processor",
        "runtime": "python3.10",
        "s3_bucket": "zappa-bucket-xxx",
        "keep_warm": false,
        "apigateway_enabled": false,
        "manage_roles": false,
        "role_name": "LabRole",
        "events": [
            {
                "function": "proyecto1.app",
                "event_source": {
                    "arn": "arn:aws:s3:::landingcasas211",
                    "events": ["s3:ObjectCreated:*"]
                }
            }
        ] ,
        "environment_variables": {
            "AWS_ACCESS_KEY_ID": "${{ secrets.AWS_ACCESS_KEY_ID }}",
            "AWS_SECRET_ACCESS_KEY": "${{ secrets.AWS_SECRET_ACCESS_KEY }}",
            "AWS_SESSION_TOKEN": "${{ secrets.AWS_SESSION_TOKEN }}"
        }
    }
}




